  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)?$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "msx",
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node",
      "gql",
      "graphql"
    ],
    "verbose": true,
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[./a-zA-Z0-9$_-]+\\.png$": "RelativeImageStub",
      "^.+\\.(scss|css|less)$": "<rootDir>/config/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/__tests__/lib/set_jest_constant.ts",
      "<rootDir>/__tests__/lib/mock_environment.ts",
      "<rootDir>/__tests__/lib/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/coverage/",
      "/__tests__/lib/",
      "/__tests__/actions/search-actions.test.ts",
      "/__tests__/actions/workspace-actions.test.ts",
      "/__tests__/actions/published-entry-actions.test.ts",
      "/__tests__/actions/content_episode-actions.test.ts",
      "/__tests__/panels/SeasonDetailPanel.test.tsx",
      "/__tests__/actions/selection-actions.test.ts",
      "/__tests__/actions/asset-actions.test.ts",
      "/__tests__/reducers/search-reducer.test.ts",
      "/__tests__/reducers/ui-state-reducer.test.ts",
      "/__tests__/actions/season-actions.test.ts",
      "/__tests__/panels/ChannelDetailPanel.test.tsx",
      "/__tests__/panels/SeasonListPanel.test.tsx",
      "/__tests__/panels/AssetListPanel.test.tsx",
      "/__tests__/panels/ContentEpisodeListPanel.test.tsx",
      "/__tests__/panels/BlockListPanel.test.tsx",
      "/__tests__/panels/AssetDetailPanel.test.tsx"
    ],
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageDirectory": "<rootDir>/__test-report__/coverage/",
    "coveragePathIgnorePatterns": [
      "node_modules",
      "flow-typed",
      "doc",
      "<rootDir>/src/client/utils/i18n.js"
    ],
    "modulePathIgnorePatterns": [
      "rpmbuild",
      "/doc/"
    ],
    "globals": {
      "fetch": "fetch"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/core-js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 20,
        "lines": 35,
        "statements": 35
      }
    }
  },
  "jest-html-reporter": {
    "pageTitle": "Your test suite",
    "outputPath": "__test-report__/index.html",
    "includeFailureMsg": false
  }
